	<section>
					<div class="slide-title">
						<h2>Mapeamento $s$ em $z$</h2>
					</div>
					<div class="slide-content">
						<center>
							<p class="lightup">$$z=e^{sT}</s>$$</p>
							
						</center>

						<center class="fragment">
							$$
							s=\sigma+j\omega
							$$
						</center>

						<center class="fragment">
							$$
							z=e^{(\sigma+j\omega)T}=e^{\sigma T}e^{j\omega T}
							$$
						</center>

					</div>
	
				</section>


				<section>
					<div class="slide-title">
						<h2>Mapeamento $s$ em $z$</h2>
					</div>
					<div class="slide-content">
				
						<center>
							$
							z=e^{(\sigma+j\omega)T}\Bigr|_{\sigma=0}=e^{j\omega T}
							$
							<img src="../images/map0.png" width="1100">
						</center>
						

					</div>
	
				</section>


				<section>
					<div class="slide-title">
						<h2>Mapeamento $s$ em $z$</h2>
					</div>
					<div class="slide-content">
				
						<center>
							$
							z=e^{(\sigma+j\omega)T}\Bigr|_{\sigma=-0.5}=e^{-0.5 T}e^{j\omega T}
							$
							<img src="../images/map1.png" width="1100">
						</center>
						

					</div>
	
				</section>


				<section>
					<div class="slide-title">
